package com.iss.stu;

public class Cal {
	private int a;
	public int b;
	
	public int sum(int a,int b){
		
		return a+b;
	}

	public int getA() {
		return a;
	}

	public void setA(int a) {
		this.a = a;
	}

	public int getB() {
		return b;
	}

	public void setB(int b) {
		this.b = b;
	}

	public Cal(int a, int b) {
		super();
		System.out.println(a+b);
	}

	public Cal() {
		super();
		
	 System.out.println("我是构造器");
	}
	1
	
}
